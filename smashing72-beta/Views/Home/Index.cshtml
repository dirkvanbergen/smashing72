@if (ViewBag.Content.DataType == "HomePage")
{
    <smashing-homepage :page="appData.contentPage" :html="appData.html"></smashing-homepage>
}
else if (ViewBag.Content.DataType == "ContentPage")
{
    <smashing-contentpage :page="appData.contentPage" :html="appData.html"></smashing-contentpage>
}
else if (ViewBag.Content.DataType == "TeamPage")
{
    <smashing-teampage :page="appData.contentPage" :html="appData.html"></smashing-teampage>
}
else if (ViewBag.Content.DataType == "ArchivePage")
{
    <smashing-archivepage :page="appData.contentPage" :html="appData.html"></smashing-archivepage>
}